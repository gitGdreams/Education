# Challenge
print("Hi user!!")
score = input("Please input your score:")
score = int(score)

if score >= 90:
    print("Grade A")
elif score >= 80:
    print("Grade B")
elif score >= 70:
    print("Grade C")
elif score >= 60:
    print("Grade D")
else:
    print("You have FFFFFFFFailed the city!!")




















